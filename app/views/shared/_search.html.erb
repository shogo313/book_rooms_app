<div>
  <%= form_with url: searches_index_path, method: :get, local: true do |f| %>
    <%= f.search_field :word_name_detail, placeholder: "キーワードで検索", value: params[:word_name_detail]%>
    <%= f.search_field :word_area, placeholder: "エリアで検索", value: params[:word_area]%>
    <%= f.submit '検索' %>
  <% end %>
</div>
